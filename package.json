{
  "name": "carmentis-wallet-extension",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tailwind": "npx tailwindcss -i ./entrypoints/popup/input.css -o ./entrypoints/popup/output.css --watch",
    "dev": "wxt --mv3",
    "dev:firefox": "wxt -b firefox --mv2",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "open:fake:web": "npx live-server --entry-file=tests/index.html"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.3",
    "react": "^18.2.0",
    "react-chrome-extension-router": "^1.4.0",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-secure-storage": "^1.3.2",
    "react-tag-input": "^6.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-tag-input": "^6.6.6",
    "@vitejs/plugin-react": "^4.2.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3",
    "wxt": "^0.17.0"
  }
}
